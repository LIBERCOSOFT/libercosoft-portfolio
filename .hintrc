{
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!.git/**",
        "!node_modules/**"
      ]
    }
  },
  "extends": [
    "development"
  ],
  "formatters": [
    "stylish"
  ],
  "hints": [
    "button-type",
    "disown-opener",
    "meta-charset-utf-8",
    "meta-viewport",
    "no-inline-styles:error"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8"
  ]
}