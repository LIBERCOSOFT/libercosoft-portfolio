{
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!.git/**",
        "!node_modules/**"
      ]
    }
  },
  "extends": [
    "development"
  ],
  "formatters": [
    "stylish"
  ],
  "hints": {
    "html-checker": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8"
  ]
}